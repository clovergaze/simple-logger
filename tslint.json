{
  "extends": "tslint:recommended",
  "rules": {
    "max-classes-per-file": false,
    "no-namespace": false,
    "no-unused-expression": false,
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ]
  }
}
